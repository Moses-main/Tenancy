# TENANCY Protocol Environment Variables
# Copy this to .env in the root directory

# ============================================================================
# PRIVY AUTHENTICATION
# ============================================================================
# Get from https://dashboard.privy.io
VITE_PRIVY_APP_ID=your_privy_app_id

# ============================================================================
# BLOCKCHAIN - Your deployer wallet (DO NOT COMMIT TO VERSION CONTROL)
# ============================================================================
PRIVATE_KEY=your_private_key_here
NETWORK=base_sepolia

# RPC URLs
SEPOLIA_RPC_URL=https://rpc.sepolia.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# ============================================================================
# CHAINLINK PRICE FEEDS
# ============================================================================
# Sepolia ETH/USD: https://docs.chainlink/data-feeds/price-feeds/addresses?network=ethereum&market=eth-usd
# Base Sepolia ETH/USD: https://docs.chainlink/data-feeds/price-feeds/addresses?network=base&market=eth-usd
CHAINLINK_ETH_USD_SEPOLIA=0x694AA1769357215DE4FAC081bf1f309aDC325306
CHAINLINK_ETH_USD_BASE_SEPOLIA=0x4a5816300e0eE47A41DFcDB12A8C8bB6dD18C12

# ============================================================================
# SMART CONTRACTS - BASE SEPOLIA
# ============================================================================
VITE_PROPERTY_REGISTRY_BASE_SEPOLIA=0x...
VITE_TEN_TOKEN_BASE_SEPOLIA=0x...
VITE_YIELD_DISTRIBUTOR_BASE_SEPOLIA=0x...
VITE_PRICE_FEED_CONSUMER_BASE_SEPOLIA=0x...
VITE_MARKETPLACE_BASE_SEPOLIA=0x...
VITE_RENTAL_TOKEN_BASE_SEPOLIA=0x...

# ============================================================================
# SMART CONTRACTS - ETHEREUM SEPOLIA
# ============================================================================
VITE_PROPERTY_REGISTRY_SEPOLIA=0x...
VITE_TEN_TOKEN_SEPOLIA=0x...
VITE_YIELD_DISTRIBUTOR_SEPOLIA=0x...
VITE_PRICE_FEED_CONSUMER_SEPOLIA=0x...
VITE_MARKETPLACE_SEPOLIA=0x...

# ============================================================================
# IPFS (Pinata) - For storing property metadata and lease agreements
# ============================================================================
# Get JWT from https://app.pinata.cloud
VITE_PINATA_JWT=your_pinata_jwt
IPFS_API_KEY=your_ipfs_api_key
IPFS_API_SECRET=your_ipfs_api_secret

# ============================================================================
# CHAINLINK FUNCTIONS - For rental verification automation
# ============================================================================
# Get from https://functions.chain.link
VITE_CHAINLINK_ROUTER=0xf9B8fc078197181C841c296C876945aaa425B278
VITE_CHAINLINK_SUBSCRIPTION_ID=your_subscription_id

# ============================================================================
# WORLDCOIN/WORLD ID - For anti-sybil verification
# ============================================================================
# Get from https://developer.worldcoin.org
WORLD_PRIVATE_KEY=your_worldcoin_private_key
WORLD_RP_ID=your_relying_party_id
VITE_WORLD_ID_APP_ID=your_world_id_app_id

# ============================================================================
# BACKEND URL
# ============================================================================
VITE_BACKEND_URL=http://localhost:4010

# ============================================================================
# FRONTEND URL (for World ID verification)
# ============================================================================
VITE_FRONTEND_URL=http://localhost:5173

