# Chainlink CRE Environment Configuration
# Copy this to .env and fill in your values

# ============================================================================
# NETWORK CONFIGURATION
# ============================================================================
NETWORK=base_sepolia
BASE_SEPOLIA_RPC_URL=https://base-sepolia-rpc.publicnode.com
SEPOLIA_RPC_URL=https://rpc.sepolia.org

# ============================================================================
# WALLET - Your deployer wallet (must have funds for gas)
# ============================================================================
PRIVATE_KEY=your_private_key_here

# ============================================================================
# CONTRACT ADDRESSES - Base Sepolia
# ============================================================================
PROPERTY_REGISTRY_ADDRESS=0x8f77c2BD2132727327B27164cDec4ccaA2083f7C
YIELD_DISTRIBUTOR_ADDRESS=0xd7c3c5e900Bd95653FA65b660a94625E1ddbBDA1
TEN_TOKEN_ADDRESS=0x539bd9076cB447Da9c88e722052293dD3394b536
PRICE_FEED_CONSUMER_ADDRESS=0xc8C6ecAA0287310bb8B0c9BE71253E758702b541

# ============================================================================
# CHAINLINK PRICE FEEDS
# ============================================================================
# Base Sepolia ETH/USD: https://docs.chainlink/data-feeds/price-feeds/addresses?network=base&market=eth-usd
# Sepolia ETH/USD: https://docs.chainlink/data-feeds/price-feeds/addresses?network=ethereum&market=eth-usd
ETH_USD_PRICE_FEED=0x4a5816300e0eE47A41DFcDB12A8C8bB6dD18C12
ETH_USD_SEPOLIA_PRICE_FEED=0x694AA1769357215DE4FAC081bf1f309aDC325306

# ============================================================================
# CHAINLINK FUNCTIONS (for automated verification)
# ============================================================================
CHAINLINK_ROUTER=0xf9B8fc078197181C841c296C876945aaa425B278
CHAINLINK_SUBSCRIPTION_ID=6273

# ============================================================================
# WORLD ID (for anti-sybil - optional)
# ============================================================================
WORLD_COIN_PRIVATE_KEY=your_worldcoin_private_key
WORLD_RP_ID=your_relying_party_id
WORLD_ID_APP_ID=your_world_id_app_id

# ============================================================================
# PAYMENT API (for rent verification)
# ============================================================================
PAYMENT_API_URL=http://localhost:3000
PAYMENT_API_KEY=your_api_key

# ============================================================================
# IPFS (Pinata) - For storing verification proofs
# ============================================================================
PINATA_JWT=your_pinata_jwt

# ============================================================================
# SERVER CONFIG
# ============================================================================
PORT=3000
NODE_ENV=development
