{
  "name": "tenancy-cre-workflow",
  "version": "1.0.0",
  "description": "Chainlink CRE workflow for rental payment verification and yield distribution",
  "type": "module",
  "scripts": {
    "simulate": "cre workflow simulate . --target staging-settings --env .env",
    "simulate:broadcast": "cre workflow simulate . --target staging-settings --broadcast --env .env",
    "deploy": "cre workflow deploy . --target staging-settings --env .env"
  },
  "dependencies": {
    "@chainlink/cre-sdk": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "bun-types": "^1.1.0"
  }
}
